@model List<Practice.Models.Entity.Student2>
@{
    ViewBag.Title = "Index";
}

<h2 class="center-block">Index : List of Students from Database</h2>
<a href="/Student2/Create" class="btn btn-primary">Add student</a>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Dob</th>
        <th>Gender</th>
        <th>Cgpa</th>
    </tr>
    
        @foreach(var item in Model)
            {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Dob</td>
                <td>@item.Gender</td>
                <td>@item.Cgpa</td>
            </tr>

            }
    


</table>

